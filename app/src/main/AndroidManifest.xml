<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.victorio.tiketsaya">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditProfilAct" />
        <activity android:name=".MyTicketDetail" />
        <activity android:name=".MyProfileAct" />
        <activity android:name=".SuccessBuyAct" />
        <activity android:name=".TicketCheckoutAct" />
        <activity android:name=".TicketDetailAct" />
        <activity android:name=".HomeAct" />
        <activity
            android:name=".SuccessRegisterAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterTwoAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterOneAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignInAct"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".GetStartedAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>